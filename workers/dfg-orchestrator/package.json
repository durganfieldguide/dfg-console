{
  "name": "dfg-orchestrator",
  "version": "0.1.0",
  "private": true,
  "description": "DFG workflow orchestrator - policy-controlled LLM automation",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "migrate": "wrangler d1 execute dfg-orchestrator-db --remote --file=migrations/0001_orchestrator.sql",
    "migrate:local": "wrangler d1 execute dfg-orchestrator-db --local --file=migrations/0001_orchestrator.sql"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "hono": "^4.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.22.0"
  }
}
