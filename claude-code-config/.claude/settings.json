{
  "allowedTools": [
    "Bash(npm:*)",
    "Bash(npx:*)",
    "Bash(pnpm:*)",
    "Bash(git:*)",
    "Bash(gh:*)",
    "Bash(wrangler:*)",
    "Bash(curl:*)",
    "Bash(cat:*)",
    "Bash(ls:*)",
    "Bash(cd:*)",
    "Bash(mkdir:*)",
    "Bash(rm:*)",
    "Bash(grep:*)",
    "Bash(find:*)",
    "Read",
    "Write",
    "Edit",
    "Glob",
    "Grep",
    "Task"
  ],
  "autoApprove": {
    "enabled": true,
    "patterns": [
      "Bash(npm:install)",
      "Bash(npm:run dev)",
      "Bash(npm:run build)",
      "Bash(npm:run test)",
      "Bash(npm:run type-check)",
      "Bash(npm:run lint)",
      "Bash(npx tsc:*)",
      "Bash(npx wrangler:dev)",
      "Bash(npx wrangler:deploy)",
      "Bash(git:status)",
      "Bash(git:diff)",
      "Bash(git:log)",
      "Bash(git:add:*)",
      "Bash(git:commit:*)",
      "Read",
      "Glob",
      "Grep"
    ]
  }
}
